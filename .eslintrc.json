{
  "extends": ["airbnb"],
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js"],
        "paths": ["."]
      }
    }
  },
  "plugins": ["prettier"],
  "env": {
    "node": true
  },
  "rules": {
    "prettier/prettier": "error",
    "quotes": 0,
    "no-console": "off",
    "comma-dangle": ["error", {
      "arrays": "always-multiline",
      "objects": "always-multiline",
      "imports": "always-multiline",
      "exports": "always-multiline",
      "functions": "never"
    }]
  }
}
